    "@pnp/nodejs-commonjs": "^2.15.0",
    "applicationinsights": "^2.9.1",
    "joi": "17.13.3",
    "moment": "2.29.4",
    "pino": "^9.5.0"
  },
  "overrides": {
    "adal-node": {
      "axios": "1.7.8"
    },
    "jsonwebtoken": "9.0.0",
    "node-fetch": "2.6.7",
    "semver": "7.5.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.13",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-when": "^3.6.0",
    "pino-pretty": "11.2.2",
    "standard": "17.0.0"
  },
  "homepage": "https://github.com/DEFRA/ffc-ahwr-mi-reporting",
  "repository": {
    "type": "git",
    "url": "https://github.com/DEFRA/ffc-ahwr-mi-reporting.git"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "expect",
      "test",
      "afterEach",
      "jest",
      "beforeAll",
      "afterAll"
    ]
